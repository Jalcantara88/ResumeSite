\n        <a href={project.link}>\r\n            <div className=\"bg-primary text-white rounded-bottom\">\r\n                Go To Project\r\n            </div>\r\n\r\n        </a>\r\n            \r\n              \r\n            \r\n                   \r\n        </React.Fragment>\r\n);\r\n};\r\n\r\n\r\nclass Portfolio extends Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {\r\n            projects: PROJECTS,\r\n            selectedProject: PROJECTS[0]\r\n        }\r\n    }\r\n    \r\n    \r\n\r\n\r\n    \r\n    render(){\r\n    const portfolio = this.state.projects.map(project => {\r\n        return(\r\n            <div key={project.id} className=\"col-3 col-md col-lg m-0 p-0 mx-auto\" onClick={() => this.setState({selectedProject: project})}>\r\n                <RenderPortfolioItem  item={project}/>\r\n            </div>\r\n        );\r\n    });\r\n\r\n    return (\r\n        <div className=\"container-fuid bg-dark py-3\" id=\"folio\">\r\n            <div className=\"row\">\r\n                <div className=\"col-12\">\r\n                    <h3 className=\"text-center text-muted\">PROJECTS</h3>\r\n                </div>\r\n                \r\n            </div>\r\n            <div className=\"row\">\r\n                <div className=\"col-10 col-lg-6 mx-auto\">\r\n                    <RenderSelectedProject project={this.state.selectedProject}/>\r\n                </div>\r\n            </div>\r\n            <div className=\"row justify-content-center mt-3\">\r\n                <div className=\"col-9 col-md-10 col-lg-6 mx-auto\">\r\n                    <div className=\"row\">\r\n                        {portfolio}\r\n                    </div>\r\n                    \r\n                </div>\r\n                \r\n            </div>\r\n        </div>\r\n    );\r\n    }\r\n};\r\n\r\nexport default Portfolio;","C:\\Users\\Undead Gems\\Desktop\\nuCamp\\3-React\\react-portfolio\\src\\redux\\projectsReducer.js",[],"C:\\Users\\